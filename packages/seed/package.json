{
    "name": "@self/seed",
    "type": "module",
    "version": "0.0.1",
    "source": "src/index.ts",
    "exports": {
        "require": "./dist/index.cjs",
        "default": "./dist/index.modern.js"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.module.js",
    "unpkg": "./dist/index.umd.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "microbundle",
