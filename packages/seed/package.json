		"factory.ts": "1.3.0"
	},
	"devDependencies": {
		"@babel/preset-typescript": "7.18.6",
		"@preconstruct/cli": "2.2.2",
		"@types/node": "18.11.18",
		"eslint-config-custom": "workspace:*",
		"prisma": "4.8.0",
		"ts-toolbelt": "9.6.0",
		"tsconfig": "workspace:*",
		"tsx": "3.12.1",
		"typescript": "4.9.4"
	}
