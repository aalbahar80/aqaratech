{
	"name": "@self/seed",
	"version": "2.3.0",
	"main": "dist/self-seed.cjs.js",
	"module": "dist/self-seed.esm.js",
	"scripts": {
		"build": "rm -rf dist && preconstruct build",
		"format": "prettier --check --config ../../.prettierrc --ignore-path ../../.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte,css,md,yaml,yml,json,jsonc}\"",
		"lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path ../../.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte}\"",
		"lint:fix": "pnpm run lint --fix",
		"check": "tsc --noEmit"
