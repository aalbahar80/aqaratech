{
  "name": "@self/site",
  "version": "0.3.2",
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "rm -rf ./build && vite build && ./alter-problematic-chunk.sh",
    "preview": ". ../../scripts/export-env.sh ../../.env && PORT=3000 ORIGIN=$PUBLIC_SITE_URL node build/index.js",
    "check": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --ignore tests",
    "check:watch": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --ignore tests --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "custom-lint": "eslint . --ext .ts,.svelte",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "sentry:sourcemaps": "sentry-cli sourcemaps upload ./build/ --org aqaratech --project site-client --project site-server --release $(awk -F'\"' '/\"version\": \".+\"/{ print $4; exit; }' ./package.json)",
    "sentry:release": "sentry-cli releases new $(awk -F'\"' '/\"version\": \".+\"/{ print $4; exit; }' ./package.json) --org aqaratech --project site-client --project site-server",
    "health-check": "../../scripts/health-probe.sh $(awk '/^PUBLIC_SITE_URL/' ../../.env | awk -F '=' '{print $2}') 30"
  },
