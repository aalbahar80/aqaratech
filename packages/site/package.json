{
	"name": "@self/site",
	"version": "2.21.0",
	"files": [
		"build"
	],
	"scripts": {
		"dev": "vite dev --port 3000 --strictPort",
		"build": "rm -rf ./{.svelte-kit,build} && svelte-kit sync && vite build",
		"preview": "pnpm run preview:node",
		"preview:vite": "vite preview --port 3000 --strictPort",
		"preview:node": "dotenv -e ../../.env -- node build/index.js",
		"check:base": "svelte-check --compiler-warnings \"a11y-click-events-have-key-events:ignore\"",
		"check": "svelte-kit sync && pnpm run check:base --output human",
		"check:watch": "pnpm run check:base --watch",
		"check:config": "tsc --noEmit --project tsconfig.lint.json",
		"format": "task format",
		"lint": "task lint -- --cache",
		"lint:fix": "task lint -- --fix",
		"flatten-sourcemaps": "../../scripts/flatten-sourcemaps.sh",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"test": "vitest --run",
		"test:ui": "vitest --ui",
		"i18n:export:tolgee": "typesafe-i18n --no-watch && ./src/i18n/translate/export.ts",
		"i18n:export": "typesafe-i18n --no-watch && ./src/i18n/translate/export.ts --skip-send --lang en && ./src/i18n/translate/export.ts --skip-send --lang ar",
		"i18n:import": "./src/i18n/translate/import.ts --tolgee && typesafe-i18n --no-watch",
		"i18n:dev": "./src/i18n/translate/import.ts && typesafe-i18n --no-watch"
	},
	"devDependencies": {
		"@iconify/json": "2.2.35",
		"@intlify/eslint-plugin-svelte": "0.1.1",
		"@logtail/types": "0.3.0",
		"@rgossiaux/svelte-headlessui": "1.0.2",
		"@self/utils": "workspace:*",
		"@sentry/svelte": "7.43.0",
		"@sentry/tracing": "7.43.0",
		"@sveltejs/adapter-node": "1.2.2",
		"@sveltejs/kit": "1.11.0",
		"@sveltejs/pancake": "0.0.18",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/container-queries": "0.1.0",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.9",
		"@tanstack/svelte-query": "4.26.1",
		"@tanstack/svelte-table": "8.7.9",
		"@types/cookie": "0.5.1",
		"@types/d3": "7.4.0",
		"@types/debounce": "1.2.1",
		"@types/papaparse": "5.3.7",
		"autoprefixer": "10.4.14",
		"chart.js": "4.0.1",
		"chartjs-adapter-date-fns": "3.0.0",
		"clsx": "1.2.1",
		"cookie": "0.5.0",
		"d3": "7.8.2",
		"date-fns": "2.29.3",
		"debounce": "1.2.1",
		"defu": "6.1.2",
		"dotenv": "16.0.3",
		"dotenv-cli": "7.1.0",
		"eslint-config-custom": "workspace:*",
		"eslint-plugin-svelte": "2.21.0",
		"frappe-charts": "1.6.2",
		"fuse.js": "6.6.2",
		"jose": "4.13.1",
		"jspdf": "2.5.1",
		"nanoid": "4.0.1",
		"papaparse": "5.4.0",
		"postcss": "8.4.21",
		"remeda": "1.9.0",
		"rollup-plugin-visualizer": "5.9.0",
		"sorcery": "0.11.0",
		"source-map-support": "0.5.21",
		"stripe": "11.14.0",
		"svelte": "3.56.0",
		"svelte-check": "3.1.4",
		"svelte-frappe-charts": "1.10.0",
		"svelte-french-toast": "1.0.3",
		"svelte-headlessui": "0.0.15",
		"svelte-preprocess": "5.0.2",
		"tailwindcss": "3.2.7",
		"tier": "5.6.0",
		"tippy.js": "6.3.7",
		"ts-toolbelt": "9.6.0",
		"tslib": "2.5.0",
		"typescript": "5.0.1-rc",
		"typescript-svelte-plugin": "0.3.21",
		"ufo": "1.1.1",
		"unplugin-icons": "0.15.3",
		"vite": "4.1.4",
