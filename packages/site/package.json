    "nodemon": "^2.0.16",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "prisma": "^3.13.0",
    "remeda": "^0.0.32",
    "superjson": "^1.9.1",
    "svelecte": "^3.7.3",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.0",
    "svelte-fa": "^2.4.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-select": "^4.4.7",
    "tailwindcss": "^3.0.23",
    "tippy.js": "^6.3.7",
    "trpc-sveltekit": "^1.2.3",
