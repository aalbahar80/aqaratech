		"zod": "3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@logtail/node": "0.4.0",
		"@logtail/winston": "0.4.1",
		"@sentry/node": "7.51.2",
		"typesafe-i18n": "5.24.3",
		"winston": "3.8.2"
	},
	"engines": {
		"node": ">= 16"
	},
	"private": true
}
