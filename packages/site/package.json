{
  "name": "@self/site",
  "version": "0.0.1",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "build:vercel": "pnpm exec prisma generate && pnpm build --filter=@self/site... && pnpm run postbuild:vercel",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --ignore tests",
    "check:watch": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --ignore tests --watch",
    "check:nodemon": "nodemon --exec 'pnpm run check' -e 'ts svelte' --watch 'src' --delay 2",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "custom-lint": "eslint . --ext .ts,.svelte",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "postbuild:vercel": "cp ../../node_modules/.pnpm/@prisma+engines@3.13.0-17.efdf9b1183dddfd4258cd181a72125755215ab7b/node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "postinstall": "prisma generate",
    "prisma:gen": "prisma generate",
    "test": "vitest run"
  },
  "devDependencies": {
    "@felte/reporter-svelte": "^1.1.1",
    "@felte/reporter-tippy": "^1.1.1",
    "@felte/validator-zod": "^1.0.9",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@prisma/client": "^3.13.0",
    "@rgossiaux/svelte-headlessui": "^1.0.0",
    "@self/temporal": "workspace:^0.0.1",
    "@sentry/browser": "^6.19.6",
    "@sentry/node": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.39",
    "@sveltejs/adapter-vercel": "^1.0.0-next.50",
    "@sveltejs/kit": "^1.0.0-next.321",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@temporalio/client": "^0.21.0",
    "@temporalio/workflow": "^0.21.0",
    "@testing-library/svelte": "^3.1.1",
    "@trpc/client": "^9.22.0",
    "@trpc/server": "^9.22.0",
    "@types/cookie": "^0.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitest/ui": "^0.10.0",
    "algoliasearch": "^4.13.0",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.2",
    "chart.js": "^3.7.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "cookie": "^0.5.0",
    "date-fns": "^2.27.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "felte": "^1.2.0",
    "fuse.js": "^6.5.3",
    "jose": "^4.8.1",
    "jsdom": "^19.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.21",
    "logrocket": "^3.0.0",
