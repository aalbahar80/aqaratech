	},
	"type": "module",
	"dependencies": {
		"@logtail/node": "0.3.1",
		"@logtail/winston": "0.3.1",
		"@sentry/node": "7.43.0",
		"typesafe-i18n": "5.24.2",
		"winston": "3.8.2"
	},
	"engines": {
		"node": ">= 16"
	},
	"private": true
}
