{
	"name": "@self/site",
	"version": "2.0.2",
	"files": [
		"build"
	],
	"scripts": {
		"dev": "vite dev --port 3000 --strictPort",
		"build": "rm -rf ./{.svelte-kit,build} && vite build",
		"preview": "vite preview --port 3000 --strictPort",
		"preview:node": ". ../../scripts/export-env.sh ../../.env && PORT=3000 ORIGIN=$PUBLIC_SITE_URL node build/index.js",
		"check": "svelte-kit sync && svelte-check --use-new-transformation --tsconfig ./tsconfig.json --output machine --compiler-warnings \"a11y-click-events-have-key-events:ignore\"",
		"check:watch": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --ignore tests --watch",
		"check:old": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --ignore tests",
		"custom-lint": "eslint . --ext .ts,.svelte",
		"lint": "eslint --ignore-path ../../.gitignore .",
		"lint:fix": "pnpm run lint --fix",
		"flatten-sourcemaps": "../../scripts/flatten-sourcemaps.sh",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"test": "vitest --run",
		"test:ui": "vitest --ui"
	},
	"devDependencies": {
		"@iconify/json": "2.1.147",
		"@logtail/types": "0.1.12",
		"@rgossiaux/svelte-headlessui": "1.0.2",
