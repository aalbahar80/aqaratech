{
	"name": "@self/site",
	"version": "2.4.0",
	"files": [
		"build"
	],
	"scripts": {
		"dev": "vite dev --port 3000 --strictPort",
		"build": "rm -rf ./{.svelte-kit,build} && vite build",
		"preview": "vite preview --port 3000 --strictPort",
		"preview:node": ". ../../scripts/export-env.sh ../../.env && PORT=3000 ORIGIN=$PUBLIC_SITE_URL node build/index.js",
		"check:base": "svelte-check --compiler-warnings \"a11y-click-events-have-key-events:ignore\"",
		"check": "svelte-kit sync && pnpm run check:base --output human",
		"check:watch": "pnpm run check:base --watch",
		"format": "prettier --check --config ../../.prettierrc --ignore-path ../../.gitignore \"(**/*.{js,cjs,mjs,ts,cts,mts,svelte,css,md,yaml,yml,json,jsonc}|!pnpm-lock.yaml)\" \"!src/i18n/i18n-*\"",
		"lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path ../../.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte}\"",
		"lint:fix": "pnpm run lint --fix",
		"flatten-sourcemaps": "../../scripts/flatten-sourcemaps.sh",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"test": "vitest --run",
		"test:ui": "vitest --ui",
		"i18n:export": "tsx src/i18n/exporter/export.ts"
	},
	"devDependencies": {
		"@iconify/json": "2.1.157",
		"@logtail/types": "0.1.12",
		"@rgossiaux/svelte-headlessui": "1.0.2",
		"@self/utils": "workspace:*",
		"@sentry/svelte": "7.28.1",
		"@sentry/tracing": "7.28.1",
		"@sveltejs/adapter-node": "1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@sveltejs/pancake": "0.0.18",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.8",
		"@tanstack/svelte-table": "8.7.4",
		"@types/cookie": "0.5.1",
		"@types/d3": "7.4.0",
		"@types/debounce": "1.2.1",
		"@types/lodash-es": "4.17.6",
		"@types/papaparse": "5.3.5",
		"autoprefixer": "10.4.13",
		"chart.js": "4.0.1",
		"chartjs-adapter-date-fns": "3.0.0",
		"clsx": "1.2.1",
		"cookie": "0.5.0",
		"d3": "7.8.0",
		"date-fns": "2.29.3",
		"debounce": "1.2.1",
		"eslint-config-custom": "workspace:*",
		"eslint-plugin-svelte": "2.14.1",
		"frappe-charts": "1.6.2",
		"fuse.js": "6.6.2",
		"jose": "4.11.1",
		"jspdf": "2.5.1",
		"just-diff": "5.2.0",
		"lodash-es": "4.17.21",
		"papaparse": "5.3.2",
		"postcss": "8.4.20",
		"remeda": "1.3.0",
		"rollup-plugin-visualizer": "5.8.3",
		"sorcery": "0.10.0",
		"source-map-support": "0.5.21",
