{
  "name": "@self/site",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "build:vercel": "pnpm exec prisma generate && pnpm --filter=@self/site... build && pnpm run postbuild:vercel",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --ignore tests",
    "check:watch": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --ignore tests --watch",
    "check:nodemon": "nodemon --exec 'pnpm run check' -e 'ts svelte' --watch 'src' --delay 2",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "custom-lint": "eslint . --ext .ts,.svelte",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "postbuild:vercel": "find ../../node_modules/.pnpm/ -name \"*libquery_engine*\" -type f -exec cp {} .vercel_build_output/functions/node/render \\;;cp prisma/schema.prisma .vercel_build_output/functions/node/render/;ls .vercel_build_output/functions/node/render/",
    "postinstall": "prisma generate",
    "prisma:gen": "prisma generate",
    "test": "vitest run"
  },
  "devDependencies": {
    "@felte/reporter-svelte": "1.1.2",
    "@felte/reporter-tippy": "1.1.2",
    "@felte/validator-zod": "1.0.10",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@prisma/client": "3.14.0",
    "@rgossiaux/svelte-headlessui": "1.0.1",
    "@sentry/browser": "6.19.7",
    "@sentry/node": "6.19.7",
    "@sentry/tracing": "6.19.7",
    "@steeze-ui/heroicons": "1.1.1",
    "@steeze-ui/svelte-icon": "1.3.1",
    "@sveltejs/adapter-auto": "1.0.0-next.41",
    "@sveltejs/adapter-vercel": "1.0.0-next.50",
    "@sveltejs/kit": "1.0.0-next.338",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.2",
    "@testing-library/svelte": "3.1.1",
    "@trpc/client": "9.23.4",
    "@trpc/server": "9.23.4",
    "@types/cookie": "0.5.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/nodemailer": "6.4.4",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "@vitest/ui": "0.12.7",
    "autoprefixer": "10.4.7",
    "chart.js": "3.7.1",
    "chartjs-adapter-date-fns": "2.0.0",
    "cookie": "0.5.0",
    "date-fns": "2.28.0",
    "dotenv": "16.0.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "felte": "1.2.2",
    "fuse.js": "6.6.2",
    "jose": "4.8.1",
    "jsdom": "19.0.0",
    "jsonwebtoken": "8.5.1",
    "just-compare": "2.1.0",
    "logrocket": "3.0.0",
    "nanoid": "3.3.4",
    "nodemailer": "6.7.5",
    "nodemon": "2.0.16",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "prisma": "3.14.0",
    "remeda": "0.0.34",
    "superjson": "1.9.1",
    "svelecte": "3.7.7",
    "svelte": "3.48.0",
    "svelte-check": "2.7.1",
    "svelte-fa": "2.4.0",
    "svelte-preprocess": "4.10.6",
    "svelte-select": "4.4.7",
    "svelte2tsx": "0.5.10",
    "tailwindcss": "3.0.24",
    "tippy.js": "6.3.7",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "uuid": "8.3.2",
    "vitest": "0.12.7",
    "vitest-svelte-kit": "0.0.6",
    "zod": "3.16.1"
  },
  "type": "module"
}