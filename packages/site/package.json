{
	"name": "@self/site",
	"version": "2.7.0",
	"files": [
		"build"
	],
	"scripts": {
		"dev": "vite dev --port 3000 --strictPort",
		"build": "rm -rf ./{.svelte-kit,build} && svelte-kit sync && vite build",
		"preview": "pnpm run preview:node",
		"preview:vite": "vite preview --port 3000 --strictPort",
		"preview:node": ". ../../scripts/export-env.sh ../../.env && BODY_SIZE_LIMIT=$BODY_SIZE_LIMIT ORIGIN=$PUBLIC_SITE_URL PORT=3000 node build/index.js",
		"check:base": "svelte-check --compiler-warnings \"a11y-click-events-have-key-events:ignore\"",
		"check": "svelte-kit sync && pnpm run check:config && pnpm run check:base --output human",
		"check:watch": "pnpm run check:base --watch",
		"check:config": "tsc --noEmit --project tsconfig.lint.json",
		"format": "prettier --check --config ../../.prettierrc --ignore-path ../../.gitignore \"(**/*.{js,cjs,mjs,ts,cts,mts,svelte,css,md,yaml,yml,json,jsonc}|!pnpm-lock.yaml)\" \"!src/i18n/{i18n-*,ar/*}\"",
		"lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path ../../.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte}\"",
		"lint:fix": "pnpm run lint --fix",
		"flatten-sourcemaps": "../../scripts/flatten-sourcemaps.sh",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"test": "vitest --run",
		"test:ui": "vitest --ui",
		"i18n:export": "pnpm typesafe-i18n --no-watch && tsx src/i18n/tolgee/export.ts",
		"i18n:import": "tsx src/i18n/tolgee/import.ts && pnpm typesafe-i18n --no-watch"
	},
	"devDependencies": {
		"@iconify/json": "2.2.5",
		"@intlify/eslint-plugin-svelte": "0.1.1",
		"@logtail/types": "0.1.15",
		"@rgossiaux/svelte-headlessui": "1.0.2",
		"@self/utils": "workspace:*",
		"@sentry/svelte": "7.30.0",
		"@sentry/tracing": "7.30.0",
		"@sveltejs/adapter-node": "1.1.0",
		"@sveltejs/kit": "1.0.11",
		"@sveltejs/pancake": "0.0.18",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.9",
		"@tanstack/svelte-table": "8.7.6",
		"@types/cookie": "0.5.1",
		"@types/d3": "7.4.0",
		"@types/debounce": "1.2.1",
		"@types/lodash-es": "4.17.6",
		"@types/papaparse": "5.3.7",
		"autoprefixer": "10.4.13",
		"chart.js": "4.0.1",
		"chartjs-adapter-date-fns": "3.0.0",
		"clsx": "1.2.1",
		"cookie": "0.5.0",
		"d3": "7.8.1",
		"date-fns": "2.29.3",
		"debounce": "1.2.1",
		"defu": "6.1.1",
		"eslint-config-custom": "workspace:*",
		"eslint-plugin-svelte": "2.14.1",
		"frappe-charts": "1.6.2",
		"fuse.js": "6.6.2",
		"jose": "4.11.2",
		"jspdf": "2.5.1",
		"just-diff": "5.2.0",
		"lodash-es": "4.17.21",
		"papaparse": "5.3.2",
		"postcss": "8.4.21",
		"remeda": "1.3.0",
		"rollup-plugin-visualizer": "5.9.0",
		"sorcery": "0.10.0",
		"source-map-support": "0.5.21",
		"svelte": "3.55.1",
		"svelte-check": "3.0.2",
		"svelte-dnd-action": "0.9.22",
		"svelte-frappe-charts": "1.10.0",
		"svelte-french-toast": "1.0.3",
		"svelte-headlessui": "0.0.13",
		"svelte-preprocess": "5.0.0",
		"tailwindcss": "3.2.4",
		"tippy.js": "6.3.7",
		"ts-toolbelt": "9.6.0",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"typescript-svelte-plugin": "0.3.13",
		"unplugin-icons": "0.15.1",
		"vite": "4.0.4",
		"vite-plugin-iso-import": "1.0.0",
		"vitest": "0.27.1",
		"zod": "3.20.2"
	},
	"type": "module",
	"dependencies": {
		"@logtail/node": "0.1.15",
		"@logtail/winston": "0.1.15",
		"@sentry/node": "7.30.0",
		"typesafe-i18n": "5.18.1",
		"winston": "3.8.2"
	},
	"private": true
}
