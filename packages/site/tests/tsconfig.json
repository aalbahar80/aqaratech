{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$components/*": ["./src/lib/components/*"],
			"$models": ["src/lib/models/index.ts"],
			"$models/*": ["src/lib/models/*"],
			"$utils": ["src/lib/utils/index.ts"],
			"$utils/*": ["src/lib/utils/*"],
			"$routers": ["src/lib/server/trpc/routers/index.ts"],
			"$routers/*": ["src/lib/server/trpc/routers/*"],
			"$environment": ["src/environment/index.ts"],
			"$user": ["src/user/index.ts"]
		}
	},
	"include": ["./**/*.ts"]
}
