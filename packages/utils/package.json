		"lint:fix": "pnpm run lint --fix",
		"check": "tsc --noEmit",
		"test": "vitest --run",
		"test:ui": "vitest --ui"
	},
	"dependencies": {
		"date-fns": "2.29.3",
		"remeda": "1.3.0",
		"ts-toolbelt": "9.6.0",
		"winston": "3.8.2",
		"zod": "3.19.0"
	},
	"devDependencies": {
		"@logtail/types": "0.1.12",
		"@sentry/types": "7.26.0",
		"@types/node": "18.11.14",
		"@vitest/ui": "0.25.8",
		"eslint-config-custom": "workspace:*",
		"tsconfig": "workspace:*",
		"tsup": "6.5.0",
		"typescript": "4.9.4",
		"vitest": "0.25.8"
	}
}
