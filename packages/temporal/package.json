{
    "name": "@self/temporal",
    "version": "0.0.1",
    "scripts": {
        "build": "tsc --build ./tsconfig.json",
        "build:watch": "tsc --build --watch ./tsconfig.json",
        "build:cloud": "prisma generate --schema=../site/prisma/schema.prisma && pnpm run build",
        "start": "node --loader ts-node/esm ./src/worker.ts",
        "start:watch": "nodemon --watch './lib/**/*.{ts,js}' --exec 'node --loader ts-node/esm' ./src/worker.ts"
    },
    "type": "module",
    "devDependencies": {
        "@prisma/client": "^3.12.0",
        "@temporalio/worker": "^0.21.1",
        "@temporalio/workflow": "^0.21.0",
        "@tsconfig/node16": "^1.0.2",
        "@types/node": "^17.0.23",
        "date-fns": "^2.27.0",
        "nodemon": "^2.0.15",
        "prisma": "^3.12.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "temporalio": "^0.19.2"
    }
}