{
    "name": "@self/temporal",
    "version": "0.0.1",
    "scripts": {
        "build": "tsc --build ./tsconfig.json",
        "build:watch": "tsc --build --watch ./tsconfig.json",
        "build:cloud": "prisma generate --schema=../site/prisma/schema.prisma && pnpm run build",
        "start": "node --loader ts-node/esm ./src/worker.ts",
        "start:watch": "nodemon --watch './lib/**/*.{ts,js}' --exec 'node --loader ts-node/esm' ./src/worker.ts",
        "test": "vitest run"
    },
    "type": "module",
    "devDependencies": {
        "@prisma/client": "^3.12.0",
        "@temporalio/client": "^0.21.0",
        "@temporalio/worker": "^0.21.1",
        "@temporalio/workflow": "^0.21.0",
        "@tsconfig/node16": "^1.0.2",
        "@types/node": "^17.0.23",
        "@types/sinon": "^10.0.11",
        "axios": "^0.26.1",
        "date-fns": "^2.27.0",
        "ms": "3.0.0-canary.1",
        "nodemon": "^2.0.15",
        "prisma": "^3.12.0",
        "sinon": "^13.0.2",
        "ts-node": "^10.7.0",
        "twilio": "^3.76.1",
        "typescript": "^4.6.3",
        "uuid": "^8.3.2",
        "vitest": "^0.9.4"
    },
    "dependencies": {
        "temporalio": "^0.21.1"
    }
}