{
	"$schema": "https://json.schemastore.org/swcrc",
	"sourceMaps": true,
	"module": {
		"type": "commonjs"
	},
	"jsc": {
		"target": "es2022",
		"parser": {
			"syntax": "typescript",
			"decorators": true,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"keepClassNames": true,
		"baseUrl": "./",
		"paths": {
			"src/*": ["./src/*"],
			"@self/utils": ["../utils/src"]
		}
	},
	"minify": false
}
