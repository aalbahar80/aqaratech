	"files": [
		"dist",
		"prisma"
	],
	"scripts": {
		"build": "rimraf dist && nest build",
		"start": "nest start",
		"dev": "nest start --watch --debug --preserveWatchOutput",
		"seed": "./seed/create-test-db.sh",
		"start:debug": "nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/src/main.js",
		"preview": "node dist/src/main.js",
		"format": "prettier --check --config ../../.prettierrc --ignore-path ../../.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte,css,md,yaml,yml,json,jsonc}\"",
		"lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path ../../.gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte}\"",
		"lint:fix": "pnpm run lint --fix",
		"check": "tsc --noEmit --project tsconfig.build.json",
		"postinstall": "prisma generate",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"api:generate": "./get-openapi.sh && rm -rf ../site/src/api/openapi && openapi-generator-cli generate",
		"up-minio": "./seed/up-minio.sh"
	},
	"prisma": {
		"seed": "tsx ./seed/seed.ts"
	},
