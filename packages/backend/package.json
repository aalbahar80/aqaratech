{
	"name": "@self/backend",
	"version": "2.14.0",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"files": [
		"dist",
		"prisma"
	],
	"scripts": {
		"build": "rimraf dist && nest build --path tsconfig.build.json",
		"dev": "nest start --watch --preserveWatchOutput --entryFile ./backend/src/main --path tsconfig.json",
		"dev:debug": "pnpm run dev --debug",
		"dev:setup": "PUBLIC_AQARATECH_ENV=development pnpm run setup:all",
		"setup:all": "pnpm run up-minio && pnpm run seed",
		"seed": "./seed/create-test-db.sh && tsx ./seed/seed.ts",
		"start:prod": "node dist/backend/src/main.js",
		"preview": "node dist/backend/src/main.js",
		"format": "task format",
		"lint": "task lint",
		"lint:fix": "task lint -- --fix",
		"check": "tsc --noEmit --project tsconfig.build.json",
		"check:watch": "pnpm run check --watch",
		"postinstall": "prisma generate",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"api:generate": "./get-openapi.sh && rm -rf ../site/src/api/openapi && openapi-generator-cli generate",
		"up-minio": "./seed/up-minio.sh"
	},
	"prisma": {
		"seed": "tsx ./seed/seed.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.272.0",
