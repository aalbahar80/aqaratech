{
	"name": "@self/backend",
	"version": "2.27.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"files": [
		"dist",
		"prisma"
	],
	"scripts": {
		"build": "task build",
		"dev": "task start -w",
		"dev:debug": "pnpm run dev --debug",
		"dev:setup": "PUBLIC_IS_TESTING=0 pnpm run setup:all",
		"setup:all": "pnpm run up-minio && pnpm run seed",
		"seed": "./seed/create-test-db.sh && tsx ./seed/seed.ts",
		"start:prod": "node dist/backend/src/main.js",
		"preview": "task start",
		"preview:coverage": "nyc --reporter=lcov --reporter=text-summary pnpm run preview",
		"format": "task -t ../../Taskfile.yml format",
		"lint": "task -t ../../Taskfile.yml lint",
		"lint:fix": "task -t ../../Taskfile.yml lint -- --fix",
		"check": "tsc --noEmit --project tsconfig.lint.json",
		"check:watch": "pnpm run check --watch",
		"postinstall": "prisma generate",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"api:document": "GENERATE_SWAGGER=1 nest start --entryFile ./backend/src/main --path tsconfig.json",
		"api:generate": "pnpm api:document && ./get-openapi.sh && rm -rf ../site/src/api/openapi && openapi-generator-cli generate",
		"up-minio": "./seed/up-minio.sh",
		"test": "task test",
		"test:watch": "TEST_WATCH=1 task test"
	},
	"prisma": {
		"seed": "tsx ./seed/seed.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.332.0",
