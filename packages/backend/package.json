{
  "name": "@self/backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "seed": "prisma migrate reset --force",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "prisma generate",
    "oas-gen": "GENERATE_OPENAPI_SCHEMA=1 nest start"
  },
  "prisma": {
    "seed": "tsx ../seed/src/go-seed.ts"
  },
  "dependencies": {
    "@casl/ability": "5.4.4",
    "@casl/prisma": "1.0.2",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.0.4",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.4",
    "@nestjs/event-emitter": "1.3.0",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.4",
    "@nestjs/swagger": "6.0.4",
    "@nestjs/terminus": "9.0.0",
    "@prisma/client": "4.1.0",
    "@sentry/node": "7.7.0",
    "@sentry/tracing": "7.7.0",
    "cache-manager": "4.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "date-fns": "2.28.0",
    "fuse.js": "6.6.2",
    "jwks-rsa": "2.1.4",
    "lodash": "4.17.21",
    "meilisearch": "0.27.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "postmark": "3.0.12",
    "prisma": "4.1.0",
    "prom-client": "14.0.1",
    "reflect-metadata": "0.1.13",
    "remeda": "0.0.35",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "swagger-ui-express": "4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.4",
    "@self/seed": "workspace:0.0.1",
    "@types/cache-manager": "4.0.1",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.6",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.6",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "prisma-dbml-generator": "0.9.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0",
    "tsx": "3.8.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "<rootDir>/../",
      "node_modules"
    ]
  }
}