{
	"name": "@self/backend",
	"version": "1.6.3",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"files": [
		"dist",
		"prisma"
	],
	"scripts": {
		"build": "rimraf dist && nest build",
		"start": "nest start",
		"dev": "nest start --watch --debug",
		"seed": "./seed/create-test-db.sh",
		"start:debug": "nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/src/main.js",
		"preview": "node dist/src/main.js",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"lint:fix": "pnpm run lint --fix",
		"check": "tsc --noEmit",
		"test:jest": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e:default": "jest --config ./test/jest-e2e.json",
		"postinstall": "prisma generate",
		"sentry:release": "../../scripts/upload-sourcemaps.mjs",
		"api:generate": "./get-openapi.sh && rm -rf ../site/src/api/openapi && openapi-generator-cli generate"
	},
	"prisma": {
		"seed": "tsx ./seed/seed.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.188.0",
		"@aws-sdk/s3-request-presigner": "3.188.0",
		"@casl/ability": "6.3.1",
		"@casl/prisma": "1.3.1",
		"@logtail/node": "0.1.12",
		"@logtail/winston": "0.1.13",
		"@nestjs/axios": "0.1.0",
		"@nestjs/common": "9.1.4",
		"@nestjs/config": "2.2.0",
		"@nestjs/core": "9.1.4",
		"@nestjs/event-emitter": "1.3.1",
		"@nestjs/jwt": "9.0.0",
		"@nestjs/passport": "9.0.0",
		"@nestjs/platform-express": "9.1.4",
		"@nestjs/swagger": "6.1.2",
		"@nestjs/terminus": "9.1.2",
		"@ntegral/nestjs-sentry": "4.0.0",
		"@prisma/client": "4.5.0",
		"@self/utils": "workspace:2.1.0",
		"@sentry/node": "7.15.0",
		"@sentry/tracing": "7.15.0",
		"cache-manager": "4.1.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.13.2",
		"cookie-parser": "1.4.6",
		"date-fns": "2.29.3",
		"helmet": "6.0.0",
		"jwks-rsa": "2.1.5",
		"lodash": "4.17.21",
		"meilisearch": "0.27.0",
		"nest-winston": "1.7.1",
		"passport": "0.6.0",
		"passport-jwt": "4.0.0",
		"postmark": "3.0.14",
		"prisma": "4.5.0",
		"reflect-metadata": "0.1.13",
		"remeda": "1.1.0",
		"rimraf": "3.0.2",
		"rxjs": "7.5.7",
		"winston": "3.8.2",
		"zod": "3.19.1"
	},
	"devDependencies": {
		"@logtail/types": "0.1.12",
		"@nestjs/cli": "9.1.4",
		"@nestjs/schematics": "9.0.3",
		"@nestjs/testing": "9.1.4",
		"@openapitools/openapi-generator-cli": "2.5.2",
		"@self/seed": "workspace:1.2.0",
		"@tsconfig/node18-strictest": "1.0.0",
		"@types/cache-manager": "4.0.2",
		"@types/cookie-parser": "1.4.3",
		"@types/express": "4.17.14",
		"@types/jest": "29.2.0",
		"@types/js-yaml": "4.0.5",
		"@types/lodash": "4.14.186",
		"@types/multer": "1.4.7",
		"@types/node": "18.11.0",
		"@types/passport-jwt": "3.0.7",
		"@types/supertest": "2.0.12",
		"eslint-config-custom": "workspace:1.1.0",
		"jest": "29.2.1",
		"js-yaml": "4.1.0",
		"source-map-support": "0.5.21",
		"supertest": "6.3.0",
		"ts-jest": "29.0.3",
		"ts-loader": "9.4.1",
		"tsconfig-paths": "4.1.0",
		"tsx": "3.10.3",
		"typescript": "4.9.1-beta"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"moduleDirectories": [
			"<rootDir>/../",
			"node_modules"
		]
	}
}
