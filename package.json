{
  "name": "svelte-REAdmin",
  "version": "0.0.1",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "dev:only": "svelte-kit dev --host",
    "build:only": "svelte-kit build",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
    "build": "yarn run tailwind:build && yarn run build:only",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "custom-lint": "eslint . --ext .ts,.svelte",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "vercel-build": "prisma generate && yarn run build",
    "postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@felte/reporter-svelte": "^0.3.19",
    "@felte/validator-zod": "^0.3.6",
    "@pinojs/json-colorizer": "^3.0.0",
    "@prisma/client": "^3.9.2",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.9",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.22",
    "@sveltejs/kit": "^1.0.0-next.272",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "cookie": "^0.4.2",
    "cross-env": "^7.0.3",
    "cypress": "^9.3.1",
    "date-fns": "^2.27.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-tonalio": "^5.1.4",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "felte": "^0.9.0",
    "jest": "^27.5.1",
    "just-capitalize": "^3.0.1",
    "just-flatten-it": "^5.0.1",
    "just-flush": "^2.1.0",
    "just-insert": "^3.0.1",
    "just-is-empty": "^3.1.1",
    "just-map-keys": "^2.1.1",
    "just-map-values": "^3.0.2",
    "just-pluck-it": "^2.1.0",
    "just-reduce-object": "^2.0.1",
    "just-zip-it": "^3.0.2",
    "lodash-es": "^4.17.21",
    "logrocket": "^2.2.0",
    "pino": "^7.6.3",
    "pino-logflare": "^0.3.12",
    "pino-pretty": "^7.4.0",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.0.2",
    "posthog-js": "^1.17.8",
    "prettier": "^2.4.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "prisma": "^3.9.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-jester": "^2.1.5",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.3",
    "tailwindcss": "^3.0.21",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "type-fest": "^2.11.2",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "zod": "^3.11.6"
  },
  "type": "module",
  "dependencies": {}
}
