{
  "name": "re-admin-panel",
  "version": "0.0.1",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "dev:only": "svelte-kit dev",
    "build:only": "svelte-kit build",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
    "build": "yarn run tailwind:build && yarn run build:only",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "custom-lint": "eslint . --ext .ts,.svelte",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "vercel-build": "prisma generate && yarn run build",
    "postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "dev:temporal": "tsc --build --watch temporal/tsconfig.json",
    "build:temporal": "tsc --build temporal/tsconfig.json",
    "start:worker": "node --loader ts-node/esm temporal/src/worker.ts",
    "start:worker.watch": "nodemon --watch './temporal/lib/**/*.{ts,js}' --exec 'node --loader ts-node/esm' temporal/src/worker.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@felte/reporter-tippy": "^1.0.5",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.33",
    "@sveltejs/kit": "^1.0.0-next.298",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.1.0",
    "@types/cookie": "^0.4.1",
    "@types/iframe-resizer": "^3.5.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitest/ui": "^0.7.6",
    "algoliasearch": "^4.13.0",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "chart.js": "^3.7.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "concurrently": "^7.0.0",
    "cookie": "^0.4.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.27.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "fuse.js": "^6.5.3",
    "iframe-resizer": "^4.3.2",
    "jsdom": "^19.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.21",
    "logrocket": "^2.2.1",
    "ms": "3.0.0-canary.1",
    "nanoid": "^3.3.1",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.0.2",
    "posthog-js": "^1.18.0",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.11.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-fa": "^2.4.0",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.0.23",
    "tippy.js": "^6.3.7",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "twilio": "^3.75.1",
    "typescript": "^4.6.2",
    "uuid": "^8.3.2",
    "vitest": "^0.7.6"
  },
  "type": "module",
  "dependencies": {
    "@felte/reporter-svelte": "^1.0.3",
    "@felte/validator-zod": "^1.0.5",
    "@prisma/client": "^3.11.0",
    "@trpc/client": "^9.20.3",
    "@trpc/server": "^9.20.3",
    "felte": "^1.1.4",
    "superjson": "^1.8.1",
    "svelte-select": "^4.4.7",
    "temporalio": "^0.19.1",
    "trpc-sveltekit": "^1.1.9",
    "zod": "^3.14.2"
  }
}
