{
	"name": "aqtech",
	"version": "1.0.0",
	"scripts": {
		"build": "turbo run build --filter !@self/test",
		"preview": "turbo run preview --filter !@self/test",
		"dev": "turbo run dev --filter !@self/test",
		"test": "turbo run test",
		"sentry:release": "turbo run sentry:release",
		"format": "prettier --check --ignore-path .gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte,css,md,yaml,yml,json,jsonc}\" \"!pnpm-lock.yaml\" \"!packages/**/*\"",
		"format:all": "turbo run format --continue",
		"format:fix": "prettier --write --ignore-path .gitignore \"(**/*.{js,cjs,mjs,ts,cts,mts,svelte,css,md,yaml,yml,json,jsonc}|!pnpm-lock.yaml)\"",
		"lint": "eslint --max-warnings 0 --report-unused-disable-directives --ignore-path .gitignore \"**/*.{js,cjs,mjs,ts,cts,mts,svelte}\"",
		"lint:all": "TIMING=1 turbo run lint --continue",
		"lint:fix": "pnpm run -r --no-bail --parallel --aggregate-output lint:fix",
		"check": "tsc --noEmit; pnpm run check:circular",
		"check:circular": "madge --circular --extensions ts --exclude .svelte-kit --no-spinner .",
		"check:all": "turbo run check --continue",
		"ci:version": "changeset version && pnpm install --lockfile-only",
		"ci:publish": "changeset tag",
		"api:generate": "pnpm run -F @self/backend api:generate && pnpm run -F @self/test api:generate && ./scripts/add-nocheck.sh && tsx ./scripts/rm-unused-jsdoc.ts && pnpm prettier --write './packages/site/src/api/openapi/**/*.ts' './packages/test/types/**/*.ts'",
		"api:generate:all": "turbo run api:generate",
		"seed": "pnpm -F @self/backend exec prisma migrate reset --force",
		"precompute": "pnpm format:all && pnpm check:all && pnpm lint:all && pnpm build && pnpm test"
	},
	"devDependencies": {
		"@changesets/changelog-git": "0.1.14",
		"@changesets/cli": "2.26.0",
		"@types/node": "18.11.18",
		"eslint": "8.31.0",
		"eslint-config-custom": "workspace:*",
		"madge": "5.0.1",
		"prettier": "2.8.2",
		"prettier-plugin-svelte": "2.9.0",
		"prettier-plugin-tailwindcss": "0.2.1",
		"tree-kill": "1.2.2",
		"ts-morph": "17.0.1",
		"tsconfig": "workspace:*",
		"tsx": "3.12.2",
		"turbo": "1.7.0",
		"typescript": "4.9.4",
		"zx": "7.1.1"
	},
	"pnpm": {
		"patchedDependencies": {
			"@rgossiaux/svelte-headlessui@1.0.2": "patches/@rgossiaux__svelte-headlessui@1.0.2.patch"
		}
	},
	"private": true
}
