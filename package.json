{
	"name": "aqtech",
	"version": "1.0.0",
	"scripts": {
		"build": "turbo run build",
		"preview": "turbo run preview",
		"dev": "turbo run dev",
		"test": "turbo run test",
		"sentry:release": "turbo run sentry:release",
		"format": "prettier --check --ignore-path .gitignore \"(**/*.{js,ts,css,md,svelte,yaml,yml,json}|!pnpm-lock.yaml)\"",
		"format:all": "turbo run format",
		"format:fix": "prettier --write --ignore-path .gitignore \"(**/*.{js,ts,css,md,svelte,yaml,yml,json}|!pnpm-lock.yaml)\"",
		"lint": "eslint --ignore-path .gitignore ./scripts/*",
		"lint:all": "TIMING=1 turbo run lint --continue",
		"lint:fix": "pnpm run -r --no-bail --parallel --aggregate-output lint:fix",
		"check": "tsc --noEmit; pnpm run check:circular",
		"check:circular": "madge --circular --extensions ts --exclude .svelte-kit --no-spinner .",
		"check:all": "turbo run check --continue",
		"ci:version": "changeset version && pnpm install --lockfile-only",
		"ci:publish": "changeset tag",
		"api:generate": "pnpm run -F @self/backend api:generate && pnpm run -F @self/test api:generate && tsx ./scripts/rm-unused-jsdoc.ts && pnpm prettier --write './packages/site/src/api/openapi/**/*.ts' './packages/test/types/**/*.ts'",
		"api:generate:all": "turbo run api:generate",
		"seed": "pnpm -F @self/backend exec prisma migrate reset --force",
		"test-report": "pnpm -F @self/test exec playwright show-report"
	},
	"devDependencies": {
		"@changesets/changelog-git": "0.1.13",
		"@changesets/cli": "2.25.0",
		"@tsconfig/node18-strictest": "1.0.0",
		"@types/node": "18.11.9",
		"eslint": "8.27.0",
		"eslint-config-custom": "workspace:*",
		"madge": "5.0.1",
		"prettier": "2.7.1",
		"prettier-plugin-tailwindcss": "0.1.13",
		"tree-kill": "1.2.2",
		"ts-morph": "16.0.0",
		"tsx": "3.11.0",
		"turbo": "1.6.3",
		"typescript": "4.9.1-beta",
		"zx": "7.1.1"
	}
}
