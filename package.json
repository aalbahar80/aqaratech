{
	"name": "aqtech",
	"version": "1.0.0",
	"scripts": {
		"build": "turbo run build -vvv",
		"preview": "turbo run preview -vvv",
		"dev": "turbo run dev",
		"test": "turbo run test -vvv",
		"sentry:release": "turbo run sentry:release",
		"format": "prettier --check --ignore-path .gitignore \"(**/*.{js,ts,css,md,svelte,yaml,yml,json}|!pnpm-lock.yaml)\"",
		"format:all": "turbo run format",
		"format:fix": "prettier --write --ignore-path .gitignore \"(**/*.{js,ts,css,md,svelte,yaml,yml,json}|!pnpm-lock.yaml)\"",
		"lint": "eslint --ignore-path .gitignore ./scripts/*",
		"lint:all": "TIMING=1 turbo run lint --continue",
		"lint:fix": "pnpm run -r --no-bail --parallel --aggregate-output lint:fix",
		"check": "tsc --noEmit; pnpm run check:circular",
		"check:circular": "madge --circular --extensions ts --exclude .svelte-kit --no-spinner .",
		"check:all": "turbo run check --continue",
		"ci:version": "changeset version && pnpm install --lockfile-only",
		"ci:publish": "changeset tag",
		"api:generate": "pnpm run -F @self/backend api:generate && pnpm run -F @self/test api:generate && sed -i '1 s/^/\\/\\/ @ts-nocheck\\\n/' packages/site/src/api/openapi/runtime.ts && tsx ./scripts/rm-unused-jsdoc.ts && pnpm prettier --write './packages/site/src/api/openapi/**/*.ts' './packages/test/types/**/*.ts'",
		"api:generate:all": "turbo run api:generate",
		"seed": "pnpm -F @self/backend exec prisma migrate reset --force",
		"precompute": "pnpm format:all && pnpm check:all && pnpm build && pnpm test"
	},
	"devDependencies": {
		"@changesets/changelog-git": "0.1.13",
		"@changesets/cli": "2.25.2",
		"@types/node": "18.11.10",
		"eslint": "8.29.0",
		"eslint-config-custom": "workspace:*",
		"madge": "5.0.1",
		"prettier": "2.8.0",
		"prettier-plugin-svelte": "2.8.1",
		"prettier-plugin-tailwindcss": "0.2.0",
		"tree-kill": "1.2.2",
		"ts-morph": "17.0.1",
		"tsconfig": "workspace:*",
		"tsx": "3.12.1",
		"turbo": "1.6.3",
		"typescript": "4.9.3",
		"zx": "7.1.1"
	},
	"pnpm": {
		"patchedDependencies": {
			"@rgossiaux/svelte-headlessui@1.0.2": "patches/@rgossiaux__svelte-headlessui@1.0.2.patch"
		}
	}
}
