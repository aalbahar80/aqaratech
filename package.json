{
  "name": "re-admin-panel",
  "version": "0.0.1",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "dev:only": "svelte-kit dev",
    "build:only": "svelte-kit build",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
    "build": "yarn run tailwind:build && yarn run build:only",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --use-new-transformation --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "custom-lint": "eslint . --ext .ts,.svelte",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "vercel-build": "prisma generate && yarn run build",
    "postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "dev:temporal": "tsc --build --watch temporal/tsconfig.json",
    "build:temporal": "tsc --build temporal/tsconfig.json",
    "start:worker": "node --loader ts-node/esm temporal/src/worker.ts",
    "start:worker.watch": "nodemon --watch './temporal/lib/**/*.{ts,js}' --exec 'node --loader ts-node/esm' temporal/src/worker.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-beta.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.31",
    "@sveltejs/kit": "^1.0.0-next.295",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/cookie": "^0.4.1",
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitest/ui": "^0.6.0",
    "autoprefixer": "^10.4.0",
    "c8": "^7.11.0",
    "concurrently": "^7.0.0",
    "cookie": "^0.4.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.27.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-tonalio": "^5.1.5",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-svelte3": "^3.4.1",
    "fuse.js": "^6.5.3",
    "jsdom": "^19.0.0",
    "lodash-es": "^4.17.21",
    "logrocket": "^2.2.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.0.2",
    "posthog-js": "^1.17.9",
    "prettier": "^2.4.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.10.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-fa": "^2.4.0",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "twilio": "^3.75.1",
    "typescript": "^4.6.2",
    "uuid": "^8.3.2",
    "vitest": "^0.6.0"
  },
  "type": "module",
  "dependencies": {
    "@felte/reporter-svelte": "^1.0.2",
    "@felte/validator-zod": "^1.0.3",
    "@prisma/client": "^3.10.0",
    "@trpc/client": "^9.20.1",
    "@trpc/server": "^9.20.1",
    "felte": "^1.1.1",
    "superjson": "^1.8.1",
    "svelte-select": "^4.4.7",
    "temporalio": "^0.18.0",
    "trpc-sveltekit": "^1.1.9",
    "zod": "^3.13.4"
  }
}
