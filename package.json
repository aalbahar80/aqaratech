{
  "name": "aqtech",
  "version": "1.0.0",
  "scripts": {
    "build": "turbo run --filter \\!@self/docs build",
    "preview": "turbo run --filter \\!@self/docs preview",
    "preview:tunnel": "turbo run --filter \\!@self/docs preview:tunnel",
    "build:site": "turbo run build --filter=@self/site",
    "build:docs": "turbo run build --filter=@self/docs",
    "seed": "turbo run seed --filter=@self/seed --filter=@self/backend",
    "test": "pnpm run --filter=@self/test test",
    "dev": "turbo run --filter \\!@self/docs dev",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "1.4.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "swagger-stats@0.99.2": "patches/swagger-stats@0.99.2.patch"
    }
  }
}