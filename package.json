{
  "name": "svelte-gql",
  "version": "0.0.1",
  "scripts": {
    "dev:only": "svelte-kit dev",
    "build:only": "svelte-kit build",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
    "build": "yarn run tailwind:build && yarn run build:only",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "generate": "graphql-codegen --config codegen.cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@auth0/auth0-spa-js": "^1.19.3",
    "@babel/core": "^7.16.5",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/named-operations-object": "^2.2.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-urql": "^3.4.1",
    "@graphql-codegen/typescript-urql-graphcache": "^2.2.2",
    "@graphql-codegen/urql-introspection": "^2.1.0",
    "@graphql-codegen/urql-svelte-operations-store": "^1.1.8",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.4.0",
    "@sveltejs/adapter-vercel": "^1.0.0-next.31",
    "@sveltejs/kit": "^1.0.0-next.201",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@urql/devtools": "^2.0.3",
    "@urql/introspection": "^0.3.1",
    "@zerodevx/svelte-toast": "^0.6.2",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "daisyui": "^1.16.5",
    "date-fns": "^2.27.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.4.5",
    "msw": "^0.36.1",
    "msw-storybook-addon": "^1.4.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.3",
    "svelte-calendar": "^3.1.6",
    "svelte-check": "^2.2.6",
    "svelte-fa": "^2.4.0",
    "svelte-forms-lib": "^2.0.0",
    "svelte-jester": "^2.1.5",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.0",
    "tailwindcss": "^3.0.2",
    "theme-change": "^2.0.2",
    "ts-jest": "^27.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "yup": "^0.32.11"
  },
  "type": "module",
  "dependencies": {
    "@urql/exchange-graphcache": "^4.3.5",
    "@urql/svelte": "^1.3.2",
    "graphql": "^16.0.1",
    "urql": "^2.0.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
