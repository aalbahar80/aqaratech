{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".svelte-kit/**",
        "dist/**",
        "build/**",
        ".vercel_build_output/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "playwright-report",
        "test-results"
      ],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "preview": {
      "dependsOn": [
        "build"
      ]
    }
  },
  "globalDependencies": [
    "**/.env",
    "**/.env.*",
    ".env",
    ".env.test"
  ]
}