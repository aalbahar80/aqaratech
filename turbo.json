{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/dev",
    "pipeline": {
        "build": {
            "outputs": [
                ".svelte-kit/**",
                "dist/**",
                "build/**",
                ".vercel_build_output/**"
            ]
        },
        "package": {
            "outputs": [
                "package/**"
            ]
        },
        "test": {
            "dependsOn": [
                "package",
                "build"
            ],
            "outputs": [
                "playwright-report",
                "test-results"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "preview": {
            "dependsOn": [
                "build"
            ]
        }
    },
    "globalDependencies": [
        "**/.env",
        "**/.env.*",
        ".env",
        ".env.test"
    ]
}