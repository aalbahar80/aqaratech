{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", "dist/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"preview": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"//#format": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"//#lint": {
			"inputs": ["scripts/**"],
			"outputs": []
		},
		"check": {
			"outputs": []
		},
		"//#check": {
			"outputs": []
		},
		"test:e2e": {
			"dependsOn": ["^build"],
			"outputs": ["playwright-report/**", "test-results/**"]
		},
		"//#api:generate": {
			"outputs": [
				"packages/site/src/api/openapi/**",
				"packages/test/types/api/**"
			],
			"inputs": ["packages/backend/openapi.yaml"]
		},
		"flatten-sourcemaps": {
			"inputs": [
				"build/**",
				"!build/client/**",
				"../../upload-sourcemaps.mjs",
				"../../scripts/flatten-sourcemaps.sh"
			],
			"outputs": ["build-temp/**"],
			"dependsOn": ["build"]
		}
	}
}
