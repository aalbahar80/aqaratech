{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", "dist/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"preview": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"//#format": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"//#lint": {
			"inputs": ["scripts/**"],
			"outputs": []
		},
		"check": {
			"outputs": []
		},
		"//#check": {
			"outputs": []
		},
		"//#test:e2e": {
			"outputs": [
				"packages/test/playwright-report/**",
				"packages/test/test-results/**"
			]
		},
		"//#api:generate": {
			"outputs": [
				"packages/site/src/api/openapi/**",
				"packages/test/types/api/**"
			],
			"inputs": ["packages/backend/openapi.yaml"]
		},
		"flatten-sourcemaps": {
			"dependsOn": ["build"],
			"outputs": ["build/**"]
		}
	}
}
