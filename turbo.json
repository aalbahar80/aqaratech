{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", "dist/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"preview": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"format": {
			"outputs": []
		},
		"//#format": {
			"inputs": ["*", "!packages/**/*", "!node_modules/**/*"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"//#lint": {
			"inputs": ["**", "!packages/**"],
			"outputs": []
		},
		"check": {
			"outputs": []
		},
		"//#check": {
			"outputs": []
		},
		"test": {
			"inputs": ["tests/**/*.ts", "../*/src/**", "playwright.config.ts"],
			"outputs": ["playwright-report/**", "test-results/**"]
		},
		"//#api:generate": {
			"outputs": [
				"packages/site/src/api/openapi/**",
				"packages/test/types/api/**"
			],
			"inputs": ["packages/backend/openapi.yaml"]
		}
	}
}
