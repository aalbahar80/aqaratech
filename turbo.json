{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/dev",
    "pipeline": {
        "build": {
            "outputs": [
                ".svelte-kit/**",
                "dist/**"
            ]
        },
        "package": {
            "outputs": [
                "package/**"
            ]
        },
        "test": {
            "dependsOn": [
                "package",
                "build"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        }
    }
}