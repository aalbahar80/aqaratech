{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/main",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".svelte-kit/**",
                "dist/**"
            ]
        },
        "test": {
            "dependsOn": [
                "^package"
            ],
            "outputs": [
                "dist/**",
                "package/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "package": {
            "outputs": [
                "dist/**",
                "package/**"
            ]
        }
    }
}