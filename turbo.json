{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build", "check"],
			"outputs": [".svelte-kit/**", "dist/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"preview": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"format": {
			"outputs": []
		},
		"//#format": {
			"inputs": [
				".do/**",
				".github/**",
				".changeset/**",
				".vscode/**",
				"scripts/**",
				"types/**",
				".eslintrc.js",
				".npmrc",
				".prettierrc",
				"Caddyfile",
				"docker-compose.yml",
				"pnpm-workspace.yaml",
				"readme.md",
				"tsconfig.json"
			],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"//#lint": {
			"inputs": [
				".do/**",
				".github/**",
				".changeset/**",
				".vscode/**",
				"scripts/**",
				"types/**",
				".eslintrc.js",
				".npmrc",
				".prettierrc",
				"Caddyfile",
				"docker-compose.yml",
				"pnpm-workspace.yaml",
				"readme.md",
				"tsconfig.json"
			],
			"outputs": []
		},
		"check": {
			"outputs": []
		},
		"//#check": {
			"inputs": [
				".do/**",
				".github/**",
				".changeset/**",
				".vscode/**",
				"scripts/**",
				"types/**",
				"tsconfig.json"
			],
			"outputs": []
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": [
				"playwright-report/**",
				"test-results/**",
				"storage-state/**",
				"downloads/**"
			]
		},
		"//#api:generate": {
			"outputs": [
				"packages/site/src/api/openapi/**",
				"packages/test/types/api/**"
			],
			"inputs": ["packages/backend/openapi.yaml"]
		}
	}
}
