{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", "dist/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"preview": {
			"cache": false,
			"dependsOn": ["build"]
		},
		"format": {
			"outputs": []
		},
		"//#format": {
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"//#lint": {
			"outputs": []
		},
		"check": {
			"outputs": []
		},
		"test:e2e": {
			"dependsOn": ["^build"],
			"outputs": ["playwright-report/**", "test-results/**"]
		},
		"health-check": {
			"cache": false
		},
		"@self/site#dev": {
			"cache": false,
			"dependsOn": ["@self/backend#health-check"]
		},
		"@self/site#preview": {
			"cache": false,
			"dependsOn": ["@self/backend#health-check"]
		}
	}
}
